//На ввод поступает три числа: начало ряда, шаг, количество шагов. Выведите на экран все числа полученного ряда. Пр. 1 2 5 → 1, 3, 5, 7, 9
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int start = sc.nextInt();  // начало ряда
        int step = sc.nextInt();   // шаг
        int count = sc.nextInt();  // количество шагов
        
        for (int i = 0; i < count; i++) {
            System.out.print(start + i * step);
            if (i < count - 1) System.out.print(", ");
        }
    }
}
